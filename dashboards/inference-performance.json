{
  "dashboard": {
    "title": "Inference Performance - Thesis QoS Metrics",
    "uid": "thesis-inference-performance",
    "tags": ["thesis", "inference", "qos"],
    "timezone": "browser",
    "schemaVersion": 16,
    "version": 3,
    "refresh": "5s",
    "panels": [
      {
        "id": 1,
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
        "type": "graph",
        "title": "ResNet50 Inference Latency",
        "targets": [
          {
            "expr": "rate(resnet50_inference_latency_seconds_sum{pod=~\"resnet50.*\"}[1m]) / rate(resnet50_inference_latency_seconds_count{pod=~\"resnet50.*\"}[1m]) * 1000",
            "legendFormat": "{{pod}} - Avg Latency",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(resnet50_inference_latency_seconds_bucket{pod=~\"resnet50.*\"}[1m])) * 1000",
            "legendFormat": "{{pod}} - P95",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, rate(resnet50_inference_latency_seconds_bucket{pod=~\"resnet50.*\"}[1m])) * 1000",
            "legendFormat": "{{pod}} - P99",
            "refId": "C"
          }
        ],
        "yaxes": [
          {"format": "ms", "label": "Latency"},
          {"format": "short"}
        ]
      },
      {
        "id": 2,
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
        "type": "graph",
        "title": "DistilBERT Inference Latency",
        "targets": [
          {
            "expr": "rate(distilbert_inference_latency_seconds_sum{pod=~\"distilbert.*\"}[1m]) / rate(distilbert_inference_latency_seconds_count{pod=~\"distilbert.*\"}[1m]) * 1000",
            "legendFormat": "{{pod}} - Avg Latency",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(distilbert_inference_latency_seconds_bucket{pod=~\"distilbert.*\"}[1m])) * 1000",
            "legendFormat": "{{pod}} - P95",
            "refId": "B"
          }
        ],
        "yaxes": [
          {"format": "ms", "label": "Latency"},
          {"format": "short"}
        ]
      },
      {
        "id": 3,
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
        "type": "graph",
        "title": "Whisper Inference Latency",
        "targets": [
          {
            "expr": "rate(whisper_inference_latency_seconds_sum{pod=~\"whisper.*\"}[1m]) / rate(whisper_inference_latency_seconds_count{pod=~\"whisper.*\"}[1m]) * 1000",
            "legendFormat": "{{pod}} - Avg Latency",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(whisper_inference_latency_seconds_bucket{pod=~\"whisper.*\"}[1m])) * 1000",
            "legendFormat": "{{pod}} - P95",
            "refId": "B"
          }
        ],
        "yaxes": [
          {"format": "ms", "label": "Latency"},
          {"format": "short"}
        ]
      },
      {
        "id": 4,
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
        "type": "graph",
        "title": "Comparative Latency (All Models)",
        "targets": [
          {
            "expr": "rate(resnet50_inference_latency_seconds_sum[1m]) / rate(resnet50_inference_latency_seconds_count[1m]) * 1000",
            "legendFormat": "ResNet50 Avg",
            "refId": "A"
          },
          {
            "expr": "rate(distilbert_inference_latency_seconds_sum[1m]) / rate(distilbert_inference_latency_seconds_count[1m]) * 1000",
            "legendFormat": "DistilBERT Avg",
            "refId": "B"
          },
          {
            "expr": "rate(whisper_inference_latency_seconds_sum[1m]) / rate(whisper_inference_latency_seconds_count[1m]) * 1000",
            "legendFormat": "Whisper Avg",
            "refId": "C"
          }
        ],
        "yaxes": [
          {"format": "ms", "label": "Latency"},
          {"format": "short"}
        ]
      },
      {
        "id": 5,
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16},
        "type": "graph",
        "title": "Request Throughput",
        "targets": [
          {
            "expr": "sum(rate(resnet50_inference_total[1m]))",
            "legendFormat": "ResNet50 req/s",
            "refId": "A"
          },
          {
            "expr": "sum(rate(distilbert_inference_total[1m]))",
            "legendFormat": "DistilBERT req/s",
            "refId": "B"
          },
          {
            "expr": "sum(rate(whisper_inference_total[1m]))",
            "legendFormat": "Whisper req/s",
            "refId": "C"
          }
        ],
        "yaxes": [
          {"format": "reqps", "label": "Requests/sec"},
          {"format": "short"}
        ]
      },
      {
        "id": 6,
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16},
        "type": "graph",
        "title": "Queue Depth",
        "targets": [
          {
            "expr": "resnet50_queue_depth",
            "legendFormat": "{{pod}} Queue",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Queue Depth"},
          {"format": "short"}
        ]
      },
      {
        "id": 7,
        "gridPos": {"h": 4, "w": 24, "x": 0, "y": 24},
        "type": "stat",
        "title": "Inference Statistics Summary",
        "targets": [
          {
            "expr": "sum(rate(resnet50_inference_total[5m]))",
            "legendFormat": "ResNet50 RPS",
            "refId": "A"
          },
          {
            "expr": "(rate(resnet50_inference_latency_seconds_sum[5m]) / rate(resnet50_inference_latency_seconds_count[5m])) * 1000",
            "legendFormat": "ResNet50 Latency (ms)",
            "refId": "B"
          },
          {
            "expr": "count(up{job=\"kubernetes-pods\",pod=~\"resnet50.*\"})",
            "legendFormat": "ResNet50 Pods",
            "refId": "C"
          }
        ],
        "options": {
          "graphMode": "none",
          "orientation": "horizontal"
        }
      },
      {
        "id": 8,
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 28},
        "type": "graph",
        "title": "Latency Under Different Load States",
        "targets": [
          {
            "expr": "rate(resnet50_inference_latency_seconds_sum{pod=~\"resnet50.*\"}[1m]) / rate(resnet50_inference_latency_seconds_count{pod=~\"resnet50.*\"}[1m]) * 1000",
            "legendFormat": "Pod {{pod}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "ms", "label": "Latency (ms)"},
          {"format": "short"}
        ]
      }
    ]
  }
}