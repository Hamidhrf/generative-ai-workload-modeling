{
  "dashboard": {
    "title": "GPU Performance - Thesis Data Collection",
    "uid": "thesis-gpu-performance",
    "tags": ["thesis", "gpu", "nvidia"],
    "timezone": "browser",
    "schemaVersion": 16,
    "version": 2,
    "refresh": "5s",
    "panels": [
      {
        "id": 1,
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
        "type": "graph",
        "title": "GPU Utilization",
        "targets": [
          {
            "expr": "DCGM_FI_DEV_GPU_UTIL{gpu=\"0\"}",
            "legendFormat": "GPU {{gpu}} Utilization",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "percent", "min": 0, "max": 100},
          {"format": "short"}
        ]
      },
      {
        "id": 2,
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
        "type": "graph",
        "title": "GPU Memory Usage",
        "targets": [
          {
            "expr": "DCGM_FI_DEV_FB_USED{gpu=\"0\"}",
            "legendFormat": "Used Memory (MiB)",
            "refId": "A"
          },
          {
            "expr": "DCGM_FI_DEV_FB_FREE{gpu=\"0\"}",
            "legendFormat": "Free Memory (MiB)",
            "refId": "B"
          }
        ],
        "yaxes": [
          {"format": "mbytes"},
          {"format": "short"}
        ]
      },
      {
        "id": 3,
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
        "type": "graph",
        "title": "GPU Memory Utilization (%)",
        "targets": [
          {
            "expr": "(DCGM_FI_DEV_FB_USED{gpu=\"0\"} / (DCGM_FI_DEV_FB_USED{gpu=\"0\"} + DCGM_FI_DEV_FB_FREE{gpu=\"0\"})) * 100",
            "legendFormat": "Memory Usage (%)",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "percent", "min": 0, "max": 100},
          {"format": "short"}
        ]
      },
      {
        "id": 4,
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
        "type": "graph",
        "title": "GPU Temperature",
        "targets": [
          {
            "expr": "DCGM_FI_DEV_GPU_TEMP{gpu=\"0\"}",
            "legendFormat": "Temperature (C)",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "celsius", "min": 0},
          {"format": "short"}
        ]
      },
      {
        "id": 5,
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16},
        "type": "graph",
        "title": "GPU Power Usage",
        "targets": [
          {
            "expr": "DCGM_FI_DEV_POWER_USAGE{gpu=\"0\"}",
            "legendFormat": "Power (W)",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "watt", "min": 0},
          {"format": "short"}
        ]
      },
      {
        "id": 6,
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16},
        "type": "graph",
        "title": "GPU Clock Speeds",
        "targets": [
          {
            "expr": "DCGM_FI_DEV_SM_CLOCK{gpu=\"0\"}",
            "legendFormat": "SM Clock (MHz)",
            "refId": "A"
          },
          {
            "expr": "DCGM_FI_DEV_MEM_CLOCK{gpu=\"0\"}",
            "legendFormat": "Memory Clock (MHz)",
            "refId": "B"
          }
        ],
        "yaxes": [
          {"format": "MHz", "min": 0},
          {"format": "short"}
        ]
      },
      {
        "id": 7,
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 24},
        "type": "graph",
        "title": "GPU Performance Over Time (Composite)",
        "targets": [
          {
            "expr": "DCGM_FI_DEV_GPU_UTIL{gpu=\"0\"}",
            "legendFormat": "GPU Util (%)",
            "refId": "A"
          },
          {
            "expr": "(DCGM_FI_DEV_FB_USED{gpu=\"0\"} / (DCGM_FI_DEV_FB_USED{gpu=\"0\"} + DCGM_FI_DEV_FB_FREE{gpu=\"0\"})) * 100",
            "legendFormat": "Memory Util (%)",
            "refId": "B"
          },
          {
            "expr": "DCGM_FI_DEV_POWER_USAGE{gpu=\"0\"} / 62 * 100",
            "legendFormat": "Power Util (% of 62W TDP)",
            "refId": "C"
          }
        ],
        "yaxes": [
          {"format": "percent", "min": 0, "max": 100},
          {"format": "short"}
        ]
      }
    ]
  }
}